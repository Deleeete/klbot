<Project Sdk="Microsoft.NET.Sdk">
  
  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <ProduceReferenceAssembly>true</ProduceReferenceAssembly>
    <TargetFramework>net5.0</TargetFramework>
    <AssemblyVersion>1.2.*</AssemblyVersion>
    <FileVersion>1.2</FileVersion>
    <Deterministic>false</Deterministic>
    <Copyright>KLG Science</Copyright>
    <Authors>Deleeete</Authors>
    <Company>KLG Science</Company>
    <Version>1.2</Version>
    <Platforms>AnyCPU;ARM32</Platforms>
  </PropertyGroup>
  <PropertyGroup>
    <Deterministic>False</Deterministic>
    <PackageId>com.klg.klbot</PackageId>
  </PropertyGroup>
  
  <ItemGroup>
    <None Include="..\.editorconfig" Link=".editorconfig" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" Version="13.0.1" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\klbotlib\klbotlib.csproj" />
  </ItemGroup>

  <ItemGroup>
    <Reference Include="KLBotModuleDemo">
      <HintPath>..\..\KLBotModuleDemo\KLBotModuleDemo\bin\Release\netstandard2.0\KLBotModuleDemo.dll</HintPath>
    </Reference>
  </ItemGroup>

  <ProjectExtensions><VisualStudio><UserProperties BuildVersion_StartDate="2000/1/1" BuildVersion_UseGlobalSettings="True" /></VisualStudio></ProjectExtensions>
  
</Project>
